alias gli='git ls-files -v | grep -e "^[hsmrck]"'
alias gl='git log --graph --format='\''%Cred%h%Creset %s    %C(yellow)%an | %C(cyan)%ad%Creset %C(green bold)%d%Creset'\'' --branches --decorate --date=local | less'
alias gs='git status'
alias go='git checkout'

function git-clorig {
    git status -su | grep -e"\.orig$" | cut -f2 -d" " | xargs rm
}

function git-sorig {
    git status -su | grep -e"\.orig$" | cut -f2 -d" "
}
